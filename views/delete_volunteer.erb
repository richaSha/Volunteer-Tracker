<ul class="breadcrumb">
  <li><a class= "active" href="/">Home</a></li>
  <li><a class= "active" href="#">Volunteers</a></li>
</ul>
<div class="wrapper">
  <div class="project_info">
    <h2>Volunteer List</h2>
    <ul class="project_list">
      <% @volunteer_list.each do |volunteer| %>
        <li><a href="/volunteer/<%= volunteer.id%>"><%= volunteer.name%></a>
          <form action="/valunteer" method="post" class="project_delete" style="
    display: inline-block;">
            <input type="hidden" name="_method" value="delete">
            <input type="hidden" name="name" value="<%= volunteer.name%>">
            <button type="submit" class="btn btn-info btn-xs">
              <span class="glyphicon glyphicon-trash"></span>
            </button>
          </form></li>
      <% end %>
    </ul>
</div>
</div>
